<web-app>
<welcome-file-list>
<welcome-file>index.cgi</welcome-file>
</welcome-file-list>
<servlet>
<servlet-name>logServlet</servlet-name>
<servlet-class>com.cgiindia.servlets.LoginServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>logServlet</servlet-name>
<url-pattern>/login.cgi</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>regServlet</servlet-name>
<servlet-class>com.cgiindia.servlets.RegiterServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>regServlet</servlet-name>
<url-pattern>/register.cgi</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>fwServlet</servlet-name>
<servlet-class>com.cgiindia.servlets.ForgotPWServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>fwServlet</servlet-name>
<url-pattern>/forgotpw.cgi</url-pattern>
</servlet-mapping>
</web-app>