package com.demopractice.java;

public class Demo {
	public static void main(String args[]) {
		System.out.println(Hello.b);
		Hello h = new Hello();
		System.out.println(h.x);
		h.x = 100;
		System.out.println(h.x);
		h.b = 99;
		Hello h1 = new Hello();
		h1.x=111;
		h1.b=222;
		System.out.println(h1.x);
		System.out.println(h.b);
	}
}

class Hello {
	static int b = 10;
	int x = 5;

}