package com.demo.polymorphism;

public class TestPolyMorphism {

	public static void main(String[] args) {
		Bank b=new ICICI();
		b.getRateOfInterest();
		
		
	}
	}

	class Bank{
		float getRateOfInterest(){
			return 0;
		}
	}
	
	class ICICI extends Bank{
		float getRateOfInterest(){
			return 9.8f;
		}
	}
	
	class SBI extends Bank{
		float getRateOfInterest(){
			return 9.3f;
		}
	}
	
	class Citi extends Bank{
		float getRateOfInterest(){
			return 12.99f;
		}
	}
}
