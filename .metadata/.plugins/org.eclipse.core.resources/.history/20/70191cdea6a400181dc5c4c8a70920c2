		//Intern operator
		
		String s1="Basha";
		String s2=new String("Basha");
		String s3=s2.intern();
		System.out.println(s1==s2);
		System.out.println(s1==s3);
		System.out.println(s2==s3);
		System.out.println("==========");
		String s4="OK".intern();
		String s5="OK";
		System.out.println(s4==s5);
	}
}
