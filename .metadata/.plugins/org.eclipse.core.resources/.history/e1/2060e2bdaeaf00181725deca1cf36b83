package com.demo.dynamicdispatch;

public class InstanceOfDemo {
	public static void main(String args[]){
		P pobj=new S();
		if(pobj instanceof R){
			R robj=(R)pobj;
			
			System.out.println("in if statement");
		}
	}
}


class P{}
class Q extends P{}
class R extends P{}
class S extends Q{}
class T{}
