package com.demo.methodoverriding;

public class MethodOverride {

	public static void main(String[] args) {
		
		Bullet b=new Bike();
		b.m1();
	}

}


class Bike{
	void m1(){
		System.out.println("Bike is running");
	}
}

class Bullet extends Bike{
	void m1(){
		System.out.println("Bullet running very fastly");
	}
}