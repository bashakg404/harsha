package com.demopractice.java;

 class C {
int num1;
static int num2;

public void show()
{
	System.out.println("----------");
	num2=9;
}}
public class Demo{
	
		public static void main(String args[]){
			System.out.println("----------");
			C.num2=5;
			System.out.println("----------");
			C obj1=new C();
			C obj2=new C();
			C obj3=new C();
			C obj4=new C();
			
			obj1.num1=2;
			obj2.num1=5;
			obj3.num1=7;
			obj4.num2=7;
			System.out.println("----------");
		}
	}


