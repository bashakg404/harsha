package com.datastructers.examples;

public class PatternStar {

	public static void main(String[] args) {
	
		int n=5;
	    printStars(n);
		
	}

	private static void printStars(int n) {
		
		int i, j;
		
		for(int i=0; i<n; i++){
			
			for(int j=0; j<=i; j++){
				System.out.println("* ");
			}
			System.out.println();
		}
		
	}
	}