class Student{
	int sid;
	Student(int sid){
		this.sid=sid;
	}
	public String toString(){
		return ""+sid;
	}
	public int hashCode(){
		System.out.println("hash cde");
		return sid;
	}
	public boolean equals(Object obj){
		System.out.println("equals");
		if(obj instanceof Student){
			Student st=(Student)obj;
			return this.sid==st.sid;
			
		}
		 return false;
	}
}
