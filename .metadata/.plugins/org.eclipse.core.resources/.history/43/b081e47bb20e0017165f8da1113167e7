package com.demopractice.java;

public class BlocksDemo {
public static void main(String args[]){
	

	System.out.println(Hai.a);
	System.out.println(Hai.b);
	Hai h=new Hai();
	//System.out.println(h.a);
	
}
}

class Hai{
	static int a;
	 int b;
	{
		a=10;
		System.out.println("Initialized:" +a);
	}
	static{
		b=11;
		System.out.println("Initialized static:"+b);
	}
}