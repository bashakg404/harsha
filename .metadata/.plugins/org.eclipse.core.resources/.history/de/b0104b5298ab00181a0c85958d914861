package com.demopractice.String;

public class StringToInt {

	public static void main(String[] args) {
		/* Convert string of number to an integer, float , double*/
		String str="12345";
		int i=Integer.parseInt(str);
		int add=i+3;
		System.out.println("Converted from String to an int:" + add);
		
		String st="72.3";
		float f=Float.parseFloat(st);
		float add=f+5;
		System.out.println("Converted form string to float:" + add);
	}

}
