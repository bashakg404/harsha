class SingleTon{
	public static void main(String[] args){
		Welcome we=Welcome.getWelcome();
		System.out.println(we);
		Welcome we1=Welcome.getWelcome();
		System.out.println(we1);
		
	}
}


class Welcome{
	private static Welcome  singletoninst=new Welcome();
	private Welcome(){}
	public static Welcome getWelcome(){
		return singletoninst;
	}
}