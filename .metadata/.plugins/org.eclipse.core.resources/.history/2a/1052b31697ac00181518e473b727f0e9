package com.demo.dynamicdispatch;

public class Vehicle {
	
	public static void main(String[] args){
		
		Vehicle v=new Vehicle();
		//Vehicle v1=new Car(); //not valid
		//Car c=new Car();
		
		System.out.println("In Main");
		
		class A{}
		class B extends A{}
		class C extends A{}
		class D extends B{}
		class E extends D{}
	}

	class Car extends Vehicle{
		Vehicle v=new Vehicle();
		Vehicle v1=new Car();
		Car c=new Car();
	}
	
	
}
